package New;

public class Missing_Number {
	
	
	public static int MissingNUM(int [] a) {
		
		int n=a.length;
		int total=(n*(n+1))/2;
		int sum=0;
		
		for(int i=0;i<a.length;i++) {
			 sum +=a[i];
		}
		return total -sum;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int Input[]= {1, 2, 3, 4, 6, 7, 8};

		MissingNUM(Input);

		System.out.println("hello");
	}

}
